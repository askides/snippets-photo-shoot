{
  "name": "snippets-photo-shoot",
  "displayName": "Snippets Photo Shoot",
  "description": "Extract beautiful snippets directly from your VSCode ",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.67.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.createSnippetShoot"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.createSnippetShoot",
        "title": "Create Snippet Shoot"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "extension.createSnippetShoot",
          "group": "01_snippetsPhotoShoot"
        }
      ],
      "explorer/context": [
        {
          "command": "extension.createSnippetShoot",
          "when": "true",
          "group": "01_snippetsPhotoShoot@1"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.67.0",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "eslint": "^8.16.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.2",
    "@vscode/test-electron": "^2.1.3"
  }
}
